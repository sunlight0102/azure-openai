{
    "name": "CopyS3ToAdls",
    "properties": {
        "activities": [
            {
                "name": "Copy data",
                "type": "Copy",
                "dependsOn": [],
                "policy": {
                    "timeout": "0.12:00:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                },
                "userProperties": [],
                "typeProperties": {
                    "source": {
                        "type": "BinarySource",
                        "storeSettings": {
                            "type": "AmazonS3ReadSettings",
                            "recursive": true,
                            "deleteFilesAfterCompletion": false
                        },
                        "formatSettings": {
                            "type": "BinaryReadSettings"
                        }
                    },
                    "sink": {
                        "type": "BinarySink",
                        "storeSettings": {
                            "type": "AzureBlobFSWriteSettings"
                        }
                    },
                    "enableStaging": false
                },
                "inputs": [
                    {
                        "referenceName": "s3Binary",
                        "type": "DatasetReference",
                        "parameters": {
                            "BucketName": {
                                "value": "@pipeline().parameters.BucketName",
                                "type": "Expression"
                            },
                            "FileName": {
                                "value": "@pipeline().parameters.FileName",
                                "type": "Expression"
                            }
                        }
                    }
                ],
                "outputs": [
                    {
                        "referenceName": "adlsgen2binary",
                        "type": "DatasetReference"
                    }
                ]
            },
            {
                "name": "Call Process Events",
                "type": "AzureFunctionActivity",
                "dependsOn": [
                    {
                        "activity": "Copy data",
                        "dependencyConditions": [
                            "Succeeded"
                        ]
                    }
                ],
                "policy": {
                    "timeout": "0.12:00:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                },
                "userProperties": [],
                "typeProperties": {
                    "functionName": {
                        "value": "@concat('ProcessEvents?fileName=', uriComponentToString(pipeline().parameters.FileName))",
                        "type": "Expression"
                    },
                    "method": "GET",
                    "body": ""
                },
                "linkedServiceName": {
                    "referenceName": "ProcessEvent",
                    "type": "LinkedServiceReference"
                }
            }
        ],
        "parameters": {
            "FileName": {
                "type": "string"
            },
            "BucketName": {
                "type": "string"
            }
        },
        "annotations": [],
        "lastPublishTime": "2023-04-27T23:07:30Z"
    },
    "type": "Microsoft.DataFactory/factories/pipelines"
}