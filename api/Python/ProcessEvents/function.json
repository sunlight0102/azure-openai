{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "retry": {
        "strategy": "fixedDelay",
        "maxRetryCount": 0,
        "delayInterval": "00:00:10"
      }
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}